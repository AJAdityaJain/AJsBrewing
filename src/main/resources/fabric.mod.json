{
	"schemaVersion": 1,
	"id": "ajsbrewing",
	"version": "${version}",
	"name": "AJ's Brewing",
	"description": "This my first Mod! Added new way to craft potions with cooking pots.\nInfinite recipe combinations and new status effects!",
	"authors": [
		"Aditya Jain!"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/AJAdityaJain/AJsBrewing"
	},
	"license": "CC0-1.0",
	"icon": "icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
          "com.ajsbrewing.AJsBrewingMod"
		],
		"client": [
			"com.ajsbrewing.AJsBrewingModClient"
		]
	},
	"mixins": [
		"ajsbrewing.mixins.json",
		{
			"config": "ajsbrewing.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=0.14.22",
		"minecraft": "~1.20.2",
		"java": ">=17",
		"fabric-api": "*"
	},
	"suggests": {
		"another-mod": "*"
	}
}
